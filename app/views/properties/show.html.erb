<div class="property-main-photo" style="background-image:url('<%= property_photo_url @property %>')">
<div class="photo-overlay">
</div>
</div>



<div class="container">
 <div classs="row">
   <div class="col-8">
      <p>
        <strong>Name:</strong>
        <%= @property.name %>
      </p>

      <p>
        <strong>Address:</strong>
        <%= @property.address %>
      </p>

      <p>
        <strong>Room:</strong>
        <%= @property.room %>
      </p>

      <p>
        <strong>Price:</strong>
        <%= @property.price %>
      </p>

      <%# <p>
        <strong>Interger:</strong>
        <%= @property.interger %>
      <%# </p> %> 

      <p>
        <strong>Bathrooms:</strong>
        <%= @property.bathrooms %>
      </p>

      <p>
        <%= image_tag @property.photo_url %>

      </p>
      <div class="col-4">
        <%= render "properties/agents_details"%>
        
      </div>


      <%= link_to 'Edit', edit_property_path(@property) %> |
      <%= link_to 'Back', properties_path %>
    </div>

  </div>
</div>
